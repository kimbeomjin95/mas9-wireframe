{
  "name": "@mas9/shared-ui",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --external react",
    "dev": "tsup src/index.ts --format esm,cjs --dts --sourcemap --external react --watch",
    "lint": "echo 'Linting disabled for TypeScript files - use type-check instead'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mas9/shared-types": "workspace:*",
    "@mas9/shared-utils": "workspace:*",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "lodash": "^4.17.21",
    "lucide-react": "^0.321.0",
    "react-toastify": "^10.0.5",
    "react-spring-bottom-sheet": "3.5.0-alpha.0"
  },
  "devDependencies": {
    "@mas9/typescript-config": "workspace:*",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "react": "^18.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}